---
import Navigation from './Navigation';
import { theme } from '../utils/theme';

let links = [
    {
        name: "Shop",
        url: `https://petdirect.co.nz/b/${theme.id}`
    }
]
---

<header>
    <Navigation
        client:visible
        name={theme.name}
        logo={`/${theme.id}/logo.svg`}
        links={links}
    />
</header>
