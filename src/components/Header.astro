---
import Navigation from './Navigation';
import { theme } from '../utils/theme';

let links = [
    {
        name: "Shop",
        url: "https://petdirect.co.nz/b/peggys-pantry"
    }
]
---

<header>
    <Navigation
        client:visible
        name={theme.name}
        logo={`${import.meta.env.BASE_URL}/${theme.id}/logo.svg`}
        links={links}
    />
</header>
